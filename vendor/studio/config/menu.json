{  	
    "applications": {
		"studio": {
			"labels": { "default": "Flow-ER Studio" }
        }
    },

    "menu/studio": {
        "tabs": ["tab/notifRules", "tab/binaryDocument"],
        "defaultTab": "tab/notifRules",
        "labels": { "default": "Le Studio" }
    },

    "tab/notifRules": {
        "menu": "menu/studio",
        "controller": "studio",
        "action": "notifRules",
        "entity": "crm_notif_rule",
        "view": "notifRules",
        "labels": {
            "default": "Notification rules",
            "fr_FR": "Règles de notifications"
        }
    },

    "tab/binaryDocument": {
        "menu": "menu/studio",
        "controller": "studio",
        "action": "notifRules",
        "entity": "document_binary",
        "view": "binaryDocument",
        "labels": { 
            "default": "Documents",
            "fr_FR": "Documents"
        }
    },

    "tab/configView": {
        "menu": "menu/studio",
        "route": "/bo/index/config_property",
        "urlParams": "view=configView",
        "labels": { "default": "Paramétrage" },
        "darkMode": true
    }
